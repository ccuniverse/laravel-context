{
  "name": "talk/laravel-context",
  "type": "library",
  "description": "Managing data in the lifecycle of a request for laravel.",
  "keywords": [
    "laravel",
    "context",
    "request"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Talk\\Context\\": "src/"
    }
  },
  "require": {
    "illuminate/support": "^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Talk\\Context\\ServiceProvider"
      ],
      "aliases": {
        "Context": "Talk\\Context\\Facade"
      }
    }
  }
}
